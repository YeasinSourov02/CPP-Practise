// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
    int dec;
    cin>>dec;
    if(dec==0){
        cout<<"0";
    }
    vector<int>ans;
    while(dec){
        ans.push_back(dec%2);
        dec=dec/2;
    }
    reverse(ans.begin(),ans.end());
    for(int i=0;i<ans.size();i++){
        cout<<ans[i];
    }
    return 0;
}
