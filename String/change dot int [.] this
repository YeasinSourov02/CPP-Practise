// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
 string s;
 cin>>s;
 int size=s.size();
 string result;
 for(int i=0;i<size;i++){
     
     if(s[i]=='.'){
         result=result+"[.]";
     }
else{         
     
     result=result+s[i];
 }
 }
 cout<<result;


    return 0;
}
