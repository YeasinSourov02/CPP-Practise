// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include<limits.h>
int main() {
string s;
cin>>s;
for(int i=1;i<s.size();i++){
    int j=i;
    //1 2 4 6
  while(j>0 && s[j]<s[j-1]){
      swap(s[j],s[j-1]);
      j--;
  }
  
    }

cout<<s;
    return 0;
}
