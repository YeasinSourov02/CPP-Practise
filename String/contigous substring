// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
  string s="ababc";
  int size=s.size();
  for(int i=0;i<size;i++){
      for(int j=i;j<size;j++){
          for(int k=i;k<=j;k++){
              cout<<s[k];
          }
          cout<<" ";
      }
  }

    return 0;
}
