
#include<bits/stdc++.h>
using namespace std;
void swap(int* a,int* b){
    int temp=*a;
    *a=*b;
    *b=temp;

}


int main(){
    int a;
    cout<<"Enter a:"<<endl;
    cin>>a;
    int b;
    cout<<"Enter b:"<<endl;
    cin>>b;
    swap(&a,&b);
    cout<<"a:"<<a<<endl;
    cout<<"b:"<<b<<endl;
}
