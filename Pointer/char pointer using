// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    char arr[5]="1234";
    
    void* ptr=arr;
    cout<<ptr<<endl;
    char* ptr1=arr;
    cout<<ptr1<<endl;
    cout<<(void*)ptr1<<endl;
    char name='a';
    char* ptr2=&name;
    cout<<ptr2;
    return 0;
}
