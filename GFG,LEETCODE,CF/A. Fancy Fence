// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    long long t;
    cin>>t;
    while(t--){
        int a;
        cin>>a;
        int result=180-a;
        if(360% result==0){
            cout<<"YES\n";
        }
        else cout<<"NO\n";
    }

    return 0;
}
