// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include<vector>
int main() {
    int n;
    cin>>n;
    
 vector<int>arr(n);
for(int i=0;i<n;i++){
    cin>>arr[i];
}
vector<int>ans(n+1,0);
for(int i=0;i<n;i++){
    ans[arr[i]]=i+1;
}
for(int i=1;i<n+1;i++){
    cout<<ans[i]<<" ";
}
    return 0;
}
