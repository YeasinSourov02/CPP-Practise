// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include<algorithm>
int main() {
  int n,k,l,c,d,p,nl,np;
  cin>>n>>k>>l>>c>>d>>p>>nl>>np;
  int total_drinks=(k*l)/nl;
  int total_slice_of_lime=c*d;
 int total_salt=p/np;
  int result=min({total_drinks,total_slice_of_lime,total_salt})/n;
  cout<<result;
    return 0;
}
