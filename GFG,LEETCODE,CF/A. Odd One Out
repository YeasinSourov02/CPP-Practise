// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
   int a,b,c;
 
   int t;
   cin>>t;
   for(int i=0;i<t;i++){
       cin>>a>>b>>c;
       if(a==b && c!=a &&c!=b){
           cout<<c<<endl;
       }
           else if(b==c && a!=b && a!=c){ 
           cout<<a<<endl;
           }
           else{ cout<<b<<endl;
           }
       }
   
    return 0;
}
