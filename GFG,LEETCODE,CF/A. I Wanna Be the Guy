// Online C++ compiler to run C++ program online
#include <iostream>
  using namespace std;
  #include<vector>
int main() {
int n;
cin>>n;
vector<int>ans(n);
int p;
cin>>p;

int a;
int count=0;
for(int i=0;i<p;i++){
    cin>>a;
    if(ans[a-1]==0) {ans[a-1]=1;
    count++;
    }
}
int q;
cin>>q;
for(int i=0;i<q;i++){
    cin>>a;
    if(ans[a-1]==0) {ans[a-1]=1;
    count++;
    }
}
if(count==n) cout<<"I become the guy.";
else  cout<<"Oh, my keyboard!";
    return 0;
}
