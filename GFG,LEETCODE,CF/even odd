// Online C++ compiler to run C++ program online #include <iostream> using namespace std; #include<vector> int main() { long long n; cin>>n; vector<long long>arr; for(long long i=1;i<=n;i=i+2){ arr.push_back(i); } for(long long i=2;i<=n;i=i+2){ arr.push_back(i); } int k; cin>>k; cout<<arr[k-1]; return 0;
