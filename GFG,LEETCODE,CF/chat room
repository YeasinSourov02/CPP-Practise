// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
   string s;
   cin>>s;
   string h="hello";
   int j=0;
   for(int i=0;i<s.size();i++){
       if(s[i]==h[j])
       j++;
       if(j==5){ cout<<"YES";
        return 0;
      
      
       }
   }
   cout<<"NO";

    return 0;
}
