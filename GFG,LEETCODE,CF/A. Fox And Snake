// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
   int m;
   int n;
   cin>>m;
   cin>>n;
   int st=0;
   for(int i=0;i<m;i++){
       for(int j=0;j<n;j++){
           if(i%2==0){
               cout<<"#";
           }
           else{
               if(st%2==0){
                   if(j==n-1){
                       cout<<"#";
                   }
                   else{
                       cout<<".";
               }
               }
               else{
                   if(j==0){
                       cout<<"#";
                   }
                   else {
                       cout<<".";
               }
               
           }
          
           }
         
       }
       
         if(i%2!=0) st++;
       cout<<endl;
   }
    return 0;
}
