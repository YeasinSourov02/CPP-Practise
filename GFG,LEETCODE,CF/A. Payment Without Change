// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
   int q;
   cin>>q;
   while(q--){
       int a,b,n,s;
       cin>>a>>b>>n>>s;
      int maxcoin=min(a,s/n);
      int rem=s-(maxcoin*n);
      if(rem<=b){
          cout<<"YES\n";
      }
      else cout<<"NO\n";
       
   }

    return 0;
}
