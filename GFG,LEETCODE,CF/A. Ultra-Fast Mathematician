// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include<string>
#include<vector>
#include<algorithm>
int main() {
  
  string s1;
  cin>>s1;
  string s2;
  cin>>s2;
  string ans;
  int size=s1.size()-1;
  for(int i=size;i>=0;i--){
      if(s1[i]!= s2[i]) ans.push_back('1');
      else ans.push_back('0');
  }
  reverse(ans.begin(),ans.end());
 cout<<ans;
    return 0;
}
