// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int totalmin=240;
    int n;
    cin>>n;
    int k;
    cin>>k;
    int count=0;
    int remainmin=totalmin-k;
    for(int i=1;i<=n;i++){
        remainmin-=5*i;
        if(remainmin>=0) count++;
        else break;
        
    }
cout<<count;
    return 0;
}
