// Online C++ compiler to run C++ program online
#include <iostream>
 using namespace std;
int main() {
//cout<<"I am a \"good\" boy";
 string adress="1.1.1";
string ans;
int index=0;
while(index<adress.size()){
    if(adress[index]=='.'){
        ans+="[.]";
    }
    else{
        ans+=adress[index];
    }
    index++;
}
cout<<ans;
    return 0;
}
