#include<bits/stdc++.h>
using namespace std;
int main(){
    
    int n;
    cin>>n;
    int sp=n-1;
    for(int i=1;i<=n;i++){
        for(int k=1;k<=sp;k++){
            cout<<" ";
        }
        char ch='E';
        for(int j=1;j<=i;j++){
            
            cout<<ch;
            ch--;
        }
        cout<<endl;
    }
}
