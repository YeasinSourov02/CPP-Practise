// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int powerc(int num,int n){
    if(n==0)
    return 1;
    return num*powerc(num,n-1);
    
}
int main() {
   int base;
   cout<<"Enter Base:\n";
   cin>>base;
   int power;
   cout<<"Enter Power:\n";
   cin>>power;
   cout<<powerc(base,power);
    return 0;
}
