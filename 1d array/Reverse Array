// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
   // 1 4 9 16
   
int n;
cout<<"Enter total array elements:";
cin>>n;
int arr[n];
cout<<"Enter Array Elements:"<<endl;
for(int i=0;i<n;i++){
 cin>>arr[i];
 
}
int i=0;
int j=n-1;
while(i<j){
    int temp=arr[i];
    arr[i]=arr[j];
    arr[j]=temp;
    i++;
    j--;
}
cout<<"Reverse Array is ";
for(int i=0;i<n;i++){
    cout<<arr[i]<<" ";
}
    return 0;
}
