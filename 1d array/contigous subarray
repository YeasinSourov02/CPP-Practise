// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int arr[]={4,-6,2,8};
for(int i=0;i<4;i++){

    for(int j=i;j<4;j++){
            cout<<"{";
        for(int k=i;k<=j;k++){
            cout<<arr[k]<<" ";
        }
        cout<<"}," ;
    }
}

    return 0;
}
