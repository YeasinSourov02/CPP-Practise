// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
   // 1 4 9 16
   
int n;
cout<<"Enter total array elements:";
cin>>n;
int arr[n];
cout<<"Enter Array Elements:"<<endl;
for(int i=0;i<n;i++){
 cin>>arr[i];
 
}
int flag=0;
int i=0;
int j=n-1;
while(i<j){
   if(arr[i]==arr[j]){
       flag=1;
   }
   i++;
   j--;
}
if(flag==1)
cout<<"Palindrom";
else{
    cout<<"not palindrom";
}
    return 0;
}
