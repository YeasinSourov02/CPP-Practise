// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
// 4 6 8 10 1 2

int main() {
   int arr[6]={4,6,8,10,2,3};
   int st=0;
   int end=6-1;
   int mid;
   int ans;
   while(st<=end){
       mid=(st+end)/2;
       if(arr[mid]>=arr[0]){
           st=mid+1;
       }
       else if(arr[mid]<arr[0]){
           ans=arr[mid];
           end=mid-1;
           
   }
   }
   cout<<"Smallest Element:"<<ans;

    return 0;
}
