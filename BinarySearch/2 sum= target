#include<bits/stdc++.h>
using namespace std;
int main(){
    int arr[5]={2,7,11,15,27};
    int start=0;
    int end=5-1;
    int target;
    cin>>target;
    int flag=0;
    while(start<end){
        if(arr[start]+arr[end]==target){
          
    flag=1;
    break;
        }
        else if(arr[start]+arr[end]>target){
            end--;
        }
        else{
            start++;
        }
    }
    if(flag==1){
        cout<<"Yeah Yeah";
    }
    else{
        cout<<"NO!";
    }

    
    }
