// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
/* ****
   ***
   **
   *
   
   */
   int row;
   cin>>row;
   int coloum;
   cin>>coloum;
   int nop=row;
   //int nop=0;
   for(int i=1;i<=row;i++){
    for(int j=1;j<=nop;j++){
      cout<<"*";  
    }   
    cout<<endl;
    nop--;
   }
    return 0;
}
