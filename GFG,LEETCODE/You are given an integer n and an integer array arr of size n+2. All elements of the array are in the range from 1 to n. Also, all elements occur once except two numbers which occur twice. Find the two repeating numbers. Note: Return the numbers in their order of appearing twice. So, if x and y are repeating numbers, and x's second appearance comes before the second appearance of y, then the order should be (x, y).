class Solution {
  public:
    // Function to find two repeated elements.
    vector<int> twoRepeated(int n, int arr[]) {
        vector<int>count(n+2,0);
        vector<int>ans;  
        for(int i=0;i<n+2;i++){
            count[arr[i]-1]++;
            if(count[arr[i]-1]==2)
            ans.push_back(arr[i]);
        }
        
      
        
      //reverse(ans.begin(),ans.end());
    return ans;
    }
};
