// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
  int n;
  cin>>n;
  int a;
  int b;
  int c;
  int output=0;
  for(int i=0;i<n;i++){
      cin>>a;
      cin>>b;
      cin>>c;
      if(a+b+c>=2){
          output++;
      }
  }
  cout<<output;
    return 0;
}
