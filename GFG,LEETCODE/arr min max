class Solution {
  public:
    pair<long long, long long> getMinMax(vector<long long int> arr) {
        // code here
        int max=INT_MIN;
        int min=INT_MAX;
        for(int i=0;i<arr.size();i++){
            if(max<arr[i]){
                max=arr[i];
            }
                if(min>arr[i])
                min=arr[i];
            }
    return make_pair(min,max); 
    }
    
};
