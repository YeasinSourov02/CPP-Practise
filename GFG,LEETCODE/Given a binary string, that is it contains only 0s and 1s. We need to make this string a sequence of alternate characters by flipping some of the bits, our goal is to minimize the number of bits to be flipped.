int minFlips (string S)
{
    int count1=0,count2=0;
    for(int i=0;i<S.size();i++){
        if(i%2==0){
            if(S[i]!='0') count1++;
            else count2++;
        }
        else{
            if(i%2!=0){
                if(S[i]!='1')
                    count1++;
                    else count2++;
                }
            
        
    }
    }
   return min(count1,count2);
   }// your code here
